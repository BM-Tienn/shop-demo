    {% do pimcore_head_script().appendFile(asset('statics/js/bootstrap.bundle.min.js')) %}
    {% do pimcore_head_script().appendFile(asset('statics/lib/wow/wow.min.js')) %} 
    {% do pimcore_head_script().appendFile(asset('statics/lib/easing/easing.min.js')) %}
    {% do pimcore_head_script().appendFile(asset('statics/lib/waypoints/waypoints.min.js')) %} 
